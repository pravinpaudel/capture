<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Top-left corner -->
    <item
        android:left="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="4dp"
                android:color="#FFFFFF" />
            <corners android:radius="12dp" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>
</layer-list>
